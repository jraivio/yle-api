{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [true, "never"],
    "trailing-comma": [
      true, {
        "multiline": "never",
        "singleline": "never",
        "imports": "never",
        "exports": "never",
        "objects": "never",
        "arrays": "never",
        "typeLiterals": "never"
      }
    ] 
  },
  "rulesDirectory": []
}
